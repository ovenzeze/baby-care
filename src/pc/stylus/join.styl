// section5
.section5
  background-image: url("../images/index-bg-02.png")
  background-size: cover
  .main
    width: 1024px
    margin: 0 auto
    text-align: center
    h3
      font-size: 22px
      margin: 30px auto
    .box
      background-size: cover
      background-color: rgba(2, 3, 4, 0.2)
      width: 1px
      height: 380px
      float: left
      margin: 20px 30px
      position: relative
      opacity: 0
      overflow: hidden
      &:hover
        img
          opacity: 0.1
      img
        height: 100%
        opacity: 0.5
        transition: all 0.5s
      .content
        position: absolute
        width: 100%
        height: 100%
        top: 0px
        left: 0px
        opacity: 0.6
        transition: all 2s
        &:hover
          opacity: 1
          i
            margin-top: 20%
            margin-bottom: 20%
          p
            display: block
        i
          display: block
          font-size: 80px
          margin-top: 30%
          margin-bottom: 30%
          transition: all 1s
        p
          line-height: 30px
          font-size: 16px
          display: none
          transition: all 2s

