// section4
.section4
  background-image: url("../images/index-bg-02.png")
  background-size: cover
  .main
    width: 1024px
    height: 500px
    margin: 0 auto
    text-align: center
    img
      width: 250px
      height: 250px
      border-radius: 30px
      margin-top: 30px
      margin-left: 10px
      transition: 0.5s
      &:hover
        box-shadow: 1px 1px 20px rgba(0, 0, 0, 1)
    .left, .center, .right
      width: 340px
      float: left
      position: relative
      text-align: center
      opacity: 0
      h3
        font-size: 24px
        text-align: center
        color: #474747
        margin-top: 40px
      h4
        font-size: 14px
        text-align: center
        color: #474747
        margin-top: 10px
      p
        font-size: 14px
        color: #474747
        text-align: center
        line-height: 25px
        margin-top: 15px
        padding: 0 25px

    .left
      left: -700px
    .center
      bottom: -700px
    .right
      right: -700px

