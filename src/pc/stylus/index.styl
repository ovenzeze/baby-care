// section1
.slide-wrapper
  .slide
    .intro
      color: white
      width: 500px
      height: 0px
      background: rgba(0, 0, 0, 0.2)
      margin: auto
      position: relative
      text-align: center
      transition: 0.5s
      display: none
      &:hover
        box-shadow 1px 1px 15px rgba(0, 0, 0, 1)
      h1, p
        text-align: center
      h1
        padding-top: 50px
        padding-bottom: 50px
      p
        line-height: 30px
        padding: 10px 30px
      input
        width: 150px
        height: 60px
        margin-top: 20px
        font-size 16px
        cursor: pointer
        border: none
        border-radius: 35px
        color: white
        background: rgba(0, 0, 0, 0.3)
        transition: 0.3s
        &:hover
          text-shadow 2px 3px 5px rgba(0, 0, 0, 1)
          box-shadow 1px 1px 15px rgba(0, 0, 0, 1)

  #slide1
    background: url("../images/index-slide-1.png") rgb(247, 215, 4)
    background-position: 50% 50%
    background-size: 90% 90%
    background-repeat: no-repeat
  #slide2
    background: url("../images/index-slide-2.png") rgb(255, 173, 82)
    background-position: 50% 50%
    background-size: cover
    background-size: 90% 90%
    background-repeat: no-repeat
  #slide3
    background: url("../images/index-slide-3.png") rgb(108, 201, 247)
    background-position: 50% 50%
    background-size: cover
    background-size: 90% 90%
    background-repeat: no-repeat

// section2
.section2
  background-image: url("../images/index-bg-03.png")
  background-size: cover
  .main
    text-align: center
    margin: 0 auto
    width: 1200px
    .title
      margin: 50px auto
      text-align: center
      position: relative
      top: -200px
      z-index: 1
      opacity: 0
      h3
        color: #474747
        font-size: 25px
      p
        margin-top: 30px
        margin-bottom: 40px
        color: rgba(234, 177, 69, 1)
        font-size: 22px
    .kinder
      display: inline-block
      border-radius: 50%
      width: 350px
      height: 350px
      cursor: pointer
      float: left
      margin-left: 70px
      position: relative
      left: -700px
      opacity: 0
      background: rgba(234, 177, 69, 0.7)
      &:hover
        box-shadow: 1px 1px 30px rgba(0, 0, 0, 0.5)
      .content
        margin: 15% auto
        width: 70%
        text-align: center
        h3
          font-size: 25px
          color: #474747
        ul
          width: 100%
          height: 70%
          margin-top: 20%
          li
            height: 45px
            line-height: 45px
            background: rgba(234, 177, 69, 0.8)
            border-radius: 35px
            margin-top: 15px
            transition: 0.5s
            &:hover
              box-shadow: 1px 1px 15px rgba(0, 0, 0, 1)

    .image-bg
      float: left
      width: 700px
      height: 400px
      left: 180px
      position: absolute
      left: 300px
      bottom: -500px
      opacity: 0
      img
        width: 100%
        height: 100%
    .earlyedu
      display: inline-block
      border-radius: 50%
      width: 350px
      cursor: pointer
      height: 350px
      float: right
      right: 100px
      position: relative
      right: -700px
      background: rgba(234, 177, 69, 0.7)
      opacity: 0
      &:hover
        box-shadow: 1px 1px 30px rgba(0, 0, 0, 0.5)
      .content
        margin: 15% auto
        width: 70%
        height: 70%
        text-align: center
        h3
          font-size: 25px
          color: #474747
        ul
          width: 100%
          height: 60%
          margin-top: 20%
          li
            height: 45px
            line-height: 45px
            background: rgba(234, 177, 69, 0.8)
            border-radius: 35px
            margin-top: 15px
            transition: 0.5s
            &:hover
              box-shadow: 1px 1px 15px rgba(0, 0, 0, 1)

